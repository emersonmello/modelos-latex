% Macro para traduzir o contador de forma a deixar em ordinal número < 10 e cardinal quando >=10
\def\traduzir#1{
\ifx @#1%
\else
\ifnum\number#1<10
\number#1º%
\else
\number#1%
\fi
\fi
}
\def\ordcard#1{\expandafter\@ordcard\csname c@#1\endcsname}
\def\@ordcard#1{\traduzir{#1}}
